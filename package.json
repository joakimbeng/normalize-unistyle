{
  "name": "normalize-unistyle",
  "version": "3.0.3",
  "description": "Normalize CSS as a Unistyle module",
  "main": "build/index.js",
  "scripts": {
    "test": "xo",
    "clean": "rimraf build/ && mkdirp build",
    "prebuild": "npm run -s clean",
    "build": "babel -d build src",
    "prepublish": "npm run -s build"
  },
  "keywords": [
    "unistyle",
    "normalize",
    "css",
    "es6",
    "es2015"
  ],
  "author": "Joakim Carlstein <joakim@klei.se>",
  "repository": "joakimbeng/normalize-unistyle",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3",
    "xo": "^0.8.0"
  },
  "xo": {
    "esnext": true,
    "ignores": [
      "build/**"
    ]
  }
}
